{
    "name": "wjiec/xet",
    "description": "Utility methods for php",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/wjiec/php-xet",
    "authors": [
        {
            "name": "Jayson Wang",
            "email": "jayson@laboys.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Xet\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Xet\\Tests\\": "tests/"
        }
    }
}
